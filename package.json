{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "Baseado no curso API em NodeJS com Clean Architecture e TDD - Mango",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node app.js",
    "test": "jest --passWithNoTests --noStackTrace --silent",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "standard": {
    "env": ["jest"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArisvaldoJRC/clean-node-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArisvaldoJRC/clean-node-api/issues"
  },
  "homepage": "https://github.com/ArisvaldoJRC/clean-node-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
